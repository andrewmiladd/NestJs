{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Nest Api",
            "args": ["${workspaceFolder}/src/main.ts"],
            "runtimeExecutable": "npm",
            "runtimeArgs": ["run", "start:debug"],
            "sourceMaps": true,
            // "envFile": "${workspaceFolder}/.env",
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal",
            "protocol": "inspector"
        }
    ]
}
